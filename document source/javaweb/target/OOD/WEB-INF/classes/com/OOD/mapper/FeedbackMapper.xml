<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.OOD.mapper.FeedbackMapper">

        <select id="addFeedback" parameterType="Feedback">
            insert into
                wifiad_feedback(name_user,email,phone,message) values(#{name_user},#{email},#{phone},#{message})
    </select>

</mapper>