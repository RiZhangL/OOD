<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.OOD.mapper.LocationMapper">

    <select id="selectLocation" parameterType="int" resultType="Location">
        select * from
            wifiad_location
        where station_id=#{station_id}
    </select>

</mapper>